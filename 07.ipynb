   "source": [
    "# Write a program that will tell whether the given year is a leap year or not"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "a04aef34",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a year 2016\n",
      "It is a leap year\n"
     ]
    }
   ],
   "source": [
    "year = int(input(\"Enter a year \"))\n",
    "\n",
    "if year % 4 == 0:\n",
    "    print(\"It is a leap year\")\n",
    "else:\n",
    "    print(\"not a leap year\")"
   ]
  },
